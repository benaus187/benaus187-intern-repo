{
  "name": "benaus187-intern-repo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll=false",
    "lint:md": "markdownlint '**/*.md' -i node_modules -i .git",
    "lint:spell": "cspell '**/*' --no-progress --gitignore --cache",
    "lint": "npm run lint:md && npm run lint:spell",
    "prepare": "husky",
    "test:cov": "jest --coverage",
    "lint:js": "eslint . --ext .js,.mjs,.cjs",
    "lint:js:fix": "eslint . --ext .js,.mjs,.cjs --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "lint-staged": {
    "*.md": [
      "markdownlint"
    ],
    "*": [
      "cspell --no-progress --no-must-find-files"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benaus187/benaus187-intern-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benaus187/benaus187-intern-repo/issues"
  },
  "homepage": "https://github.com/benaus187/benaus187-intern-repo#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "cspell": "^9.2.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2"
  }
}
